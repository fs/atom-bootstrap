"*":
  core:
    disabledPackages: [
      "gramma-selector"
      "image-view",
      "language-c",
      "language-clojure",
      "language-csharp",
      "language-go",
      "language-haml"
      "language-java",
      "language-make",
      "language-objective-c",
      "language-perl",
      "language-php",
      "language-python",
      "language-todo",
      "language-toml",
      "styleguide",
      "welcome",
    ]
    projectHome: "~/src"
    themes: [
      "one-light-ui"
      "solarized-light-syntax"
    ]
  autosave:
    enabled: true
  editor:
    fontSize: 15
    preferredLineLength: 120
    showIndentGuide: true
  "highlight-selected":
    hideHighlightOnSelectedWord: true
    highlightBackground: true
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
